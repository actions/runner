{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "ConnectionStrings": {
    "sqlite": "Data Source=Agents.db;"
  },
  "Runner.Server": {
    "GitServerUrl": "https://github.com",
    "GitApiServerUrl": "https://api.github.com",
    "GitGraphQlServerUrl": "https://api.github.com/graphql",
    "GITHUB_TOKEN": null,
    "Secrets": [
      {"Name": "SampleSecret", "Value": "SampleValue"}
    ],
    "ActionDownloadUrls": [
      {
        "TarballUrl": "https://api.github.com/repos/{0}/tarball/{1}",
        "ZipballUrl": "https://api.github.com/repos/{0}/zipball/{1}"
      }
    ]
  }
}
