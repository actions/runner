{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": ["plugin:github/recommended"],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
    "import/no-commonjs": "off",
    "filenames/match-regex": "off",
    "no-console": "off"
  }
}